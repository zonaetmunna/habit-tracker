{
  "expo": {
    "name": "habit-tracker",
    "slug": "habit-tracker",
    "version": "1.0.0",
    "scheme": "habit-tracker",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.zonaetmonna.habittracker"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "88376d04-af5a-4fea-9f4d-42d65a33778c"
      }
    }
  }
}
